{
  "name": "react-redux-typescript",
  "version": "1.1.0",
  "description": "React / Redux / TypeScript Utils",
  "repository": "https://github.com/piotrwitek/react-redux-typescript",
  "author": "Piotr Witek <piotrek.witek@gmail.com> (http://piotrwitek.github.io)",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "jsnext:main": "jsnext/index.js",
  "engines": {
    "node": ">= 0.10.x"
  },
  "scripts": {
    "prepublishOnly": "yarn run reinstall && yarn run test && yarn run build && yarn run build:jsnext",
    "reinstall": "rm -rf node_modules/ && yarn install",
    "test": "npm run lint && npm run tsc",
    "build": "rm -rf dist && tsc -p src",
    "build:jsnext": "rm -rf jsnext && tsc -p src -t 'ES2015' --outDir jsnext",
    "lint": "tslint --project src/tsconfig.json",
    "tsc": "tsc -p src --noEmit"
  },
  "dependencies": {},
  "devDependencies": {
    "tslib": "^1.5.0",
    "tslint": "^4.3.1",
    "typescript": "^2.1.5"
  },
  "keywords": [
    "react",
    "redux",
    "typescript",
    "utils"
  ]
}
