{
  "name": "react-redux-typescript",
  "version": "0.4.0",
  "description": "React / Redux / TypeScript Utils",
  "repository": "https://github.com/piotrwitek/react-redux-typescript",
  "author": "Piotr Witek <piotrek.witek@gmail.com> (http://piotrwitek.github.io)",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "jsnext:main": "es2015/index.js",
  "engines": {
    "node": ">= 0.10.x"
  },
  "scripts": {
    "prepublishOnly": "yarn run clean && yarn && yarn run test && yarn run build",
    "clean": "rm -rf node_modules/",
    "build": "tsc -p . && tsc -p . -t 'ES2015' --outDir es2015",
    "start": "npm run tsc",
    "test": "npm run lint && npm run tsc",
    "lint": "tslint '*.ts'",
    "tsc": "tsc -p . --noEmit"
  },
  "dependencies": {},
  "devDependencies": {
    "tslint": "^4.0.2",
    "typescript": "^2.1.4"
  },
  "keywords": [
    "react",
    "redux",
    "typescript",
    "utils"
  ]
}